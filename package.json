{
  "name": "minimal-auth-app",
  "version": "1.0.0",
  "description": "Minimal authentication system with Vue.js and Express",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server.js",
    "client:dev": "webpack serve --mode development",
    "build": "webpack --mode production"
  },
  "dependencies": {
    "express": "^4.18.2",
    "bcrypt": "^5.1.0",
    "jsonwebtoken": "^9.0.0",
    "sqlite3": "^5.1.6",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.1.5",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@vue/compiler-sfc": "^3.2.47",
    "babel-loader": "^9.1.2",
    "bulma": "^0.9.4",
    "concurrently": "^8.0.1",
    "css-loader": "^6.7.3",
    "html-webpack-plugin": "^5.5.1",
    "nodemon": "^2.0.22",
    "style-loader": "^3.3.2",
    "vue": "^3.2.47",
    "vue-loader": "^17.0.1",
    "webpack": "^5.82.0",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0"
  }
}