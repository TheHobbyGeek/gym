{
  "name": "crypto-trading-assistant",
  "version": "1.0.0",
  "description": "A cryptocurrency trading assistant application",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run server:dev\" \"npm run client:dev\"",
    "dev:alt": "node start-dev.js",
    "server:dev": "nodemon server.js",
    "client:dev": "wait-on http://localhost:3000 && webpack serve --config webpack.config.js --mode development",
    "build": "webpack --config webpack.config.js --mode production",
    "start": "node server.js",
    "postbuild": "echo 'Build complete - run npm start to serve the application'"
  },
  "keywords": [
    "crypto",
    "trading",
    "assistant",
    "vue",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "nodemon": "^3.0.1",
    "style-loader": "^3.3.3",
    "vue": "^3.3.4",
    "vue-loader": "^17.2.2",
    "vue-style-loader": "^4.1.3",
    "wait-on": "^7.2.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
